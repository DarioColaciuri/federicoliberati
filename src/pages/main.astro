---
import { Image } from "astro:assets";
import NavFoot from "../layouts/NavFoot.astro";
---

<NavFoot title="Federico-Liberati">
  <main class="img-container">
    <Image
      class="logomain"
      src="/logo.png"
      alt="Federico-Liberati logo"
      width={400}
      height={200}
    />
  </main>
</NavFoot>

<style>
  .img-container {
    display: flex;
    justify-content: center;
    align-items: center;
    height: 81vh;
  }

  .logomain {
    width: 260px;
  }


</style>
